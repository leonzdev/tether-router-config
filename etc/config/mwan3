
config globals 'globals'
	option mmx_mask '0x3F00'
	list rt_table_lookup '220'

config rule 'https'
	option sticky '1'
	option dest_port '443'
	option proto 'tcp'
	option use_policy 'tether_lb'
	option family 'ipv4'

config rule 'default_rule_v4'
	option dest_ip '0.0.0.0/0'
	option family 'ipv4'
	option proto 'all'
	option sticky '0'
	option use_policy 'tether_lb'

config rule 'default_rule_v6'
	option dest_ip '::/0'
	option family 'ipv6'
	option proto 'all'
	option sticky '0'
	option use_policy 'unreachable'

config interface 'tether0'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '70'
	option recovery_interval '5'
	option up '1'
	option down '3'
	list track_ip '8.8.8.8'
	list track_ip '1.1.1.1'
	option timeout '3'
	option interval '5'
	option failure_interval '3'
	list flush_conntrack 'disconnected'
	list flush_conntrack 'connected'
	list flush_conntrack 'ifup'
	list flush_conntrack 'ifdown'

config interface 'tether1'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '70'
	option recovery_interval '5'
	option down '3'
	option up '1'
	list track_ip '8.8.8.8'
	list track_ip '1.1.1.1'
	option timeout '3'
	option interval '5'
	option failure_interval '3'
	list flush_conntrack 'connected'
	list flush_conntrack 'disconnected'
	list flush_conntrack 'ifup'
	list flush_conntrack 'ifdown'

config member 'tether0_m1_w2'
	option interface 'tether0'
	option metric '1'
	option weight '2'

config member 'tether1_m1_w2'
	option interface 'tether1'
	option metric '1'
	option weight '2'

config policy 'tether_lb'
	option last_resort 'unreachable'
	list use_member 'tether0_m1_w2'
	list use_member 'tether1_m1_w2'
	list use_member 'tether2_m1_w2'
	list use_member 'tether3_m1_w2'

config interface 'tether2'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option recovery_interval '5'
	option max_ttl '70'
	option down '3'
	option up '1'
	list flush_conntrack 'connected'
	list flush_conntrack 'disconnected'
	list flush_conntrack 'ifup'
	list flush_conntrack 'ifdown'
	list track_ip '8.8.8.8'
	list track_ip '1.1.1.1'
	option timeout '3'
	option interval '5'
	option failure_interval '3'

config member 'tether2_m1_w2'
	option interface 'tether2'
	option metric '1'
	option weight '2'

config policy 'tether0_only'
	list use_member 'tether0_m1_w2'
	option last_resort 'unreachable'

config policy 'tether1_only'
	list use_member 'tether1_m1_w2'
	option last_resort 'unreachable'

config policy 'tether2_only'
	list use_member 'tether2_m1_w2'
	option last_resort 'unreachable'

config interface 'tether3'
	option enabled '1'
	option initial_state 'online'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option max_ttl '70'
	option recovery_interval '5'
	option down '3'
	option up '1'
	list flush_conntrack 'ifup'
	list flush_conntrack 'ifdown'
	list flush_conntrack 'connected'
	list flush_conntrack 'disconnected'
	list track_ip '1.1.1.1'
	list track_ip '8.8.8.8'
	option timeout '3'
	option interval '5'
	option failure_interval '3'

config member 'tether3_m1_w2'
	option interface 'tether3'
	option metric '1'
	option weight '2'

